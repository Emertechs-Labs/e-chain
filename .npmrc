# Package installation
save-exact=false
package-lock=true

# Performance
prefer-offline=true
fetch-retries=3
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000

# Security
audit=true

# Logging
loglevel=warn

# Engine strictness
engine-strict=false

# Legacy peer deps (for React 18 compatibility)
legacy-peer-deps=false
